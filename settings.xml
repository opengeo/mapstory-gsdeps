<settings>
  <!--
  <localRepository>/path/to/local/repo</localRepository>
  <offline>true</offline>
  -->
  <servers>
    <server>
     <id>mapstory</id>
     <username>[username]</username>
     <password>[password]</password>
    </server>
 </servers>
 <profiles>
  <profile>
   <id>mapstory-deploy</id>
   <properties>
     <!-- point all deployment at the mapstory repo -->
     <altDeploymentRepository>mapstory::default::dav:http://mapstory.repo.opengeo.org</altDeploymentRepository>
   </properties>
  </profile>
  <profile>
   <id>no-snapshots</id>
   <repositories>
    <repository>
     <id>opengeo</id>
     <name>opengeo</name>
     <snapshots>
      <enabled>false</enabled>
      <updatePolicy>never</updatePolicy>
     </snapshots>
     <url>http://repo.opengeo.org/</url>
    </repository>
    <repository>
     <id>osgeo</id>
     <name>Open Source Geospatial Foundation Repository</name>
     <url>http://download.osgeo.org/webdav/geotools/</url>
     <snapshots>
      <enabled>false</enabled>
      <updatePolicy>never</updatePolicy>
     </snapshots>
    </repository>
   </repositories>
  </profile>
 </profiles>
 <activeProfiles>
   <activeProfile>mapstory-deploy</activeProfile>
   <activeProfile>no-snapshots</activeProfile>
 </activeProfiles>

</settings>
